  },
  "scripts": {
    "start-server": "nodemon dist/src/server.js",
    "build-watch": "tsc --watch --preserveWatchOutput",
    "build": "tsc",
    "start": "concurrently \"npm run build-watch\" \"npm run start-server\"",
    "clean": "rm dist/* -r",
